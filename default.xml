<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="origin" fetch="ssh://git@github.com"/>
  <default revision="master" remote="origin" sync-j="4" />

  <!-- Personal Repositories -->
  <project path="build/workspace" name="Mord3rca/repo-workspace">
    <linkfile src="Makefile" dest="Makefile"/>
  </project>
  <project path="config/morderca-external" name="Mord3rca/buildroot-tree"/>
  <project path="documentation/francis" name="Mord3rca/francis"/>
  <project path="package/dyndns-updater" name="Mord3rca/dyndns-updater"/>

  <!-- 3rd party provider(s) -->
  <project path="build/buildroot" name="buildroot/buildroot"/>
</manifest>
